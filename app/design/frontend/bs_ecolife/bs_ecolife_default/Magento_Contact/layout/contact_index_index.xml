<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Contact Us</title>
    </head>
    <body>
		<referenceContainer name="page.wrapper">
            <block name="maps" before="header.container" class="Magento\Cms\Block\Block">    
                 <arguments>
                      <argument name="block_id" xsi:type="string">maps</argument>
                 </arguments>
             </block>
        </referenceContainer>      
		<referenceContainer name="main">
			<container name="contact.info" htmlTag="div" htmlClass="contact-info">
				<block class="Magento\Cms\Block\Block" name="contact-info">
					<arguments>
						<argument name="block_id" xsi:type="string">contact-info</argument>
					</arguments>
				</block>
			</container>
        </referenceContainer>
		<move element="contactForm" destination="main" before="contact.info"/>
    </body>
</page>
